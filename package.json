{
  "name": "rendergun",
  "version": "0.5.0",
  "description": "A pre-render service for client-side rendered websites",
  "repository": "https://github.com/goenning/rendergun",
  "main": "dist/index.js",
  "bin": {
    "rendergun": "dist/index.js"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsc && DEBUG=http,renderer:*,error,cache NODE_ENV=development node dist/index.js",
    "test": "jest",
    "lint": "tslint --project .",
    "build": "tsc",
    "prepublishOnly": "npm run lint && npm run test && npm run build"
  },
  "keywords": [
    "ssr",
    "puppeteer",
    "server-side-rendering",
    "docker"
  ],
  "author": "goenning <me@goenning.net>",
  "license": "MIT",
  "dependencies": {
    "compression": "1.7.3",
    "debug": "4.1.1",
    "express": "4.16.4",
    "lru-cache": "5.1.1",
    "puppeteer": "1.12.2"
  },
  "devDependencies": {
    "@types/lru-cache": "5.1.0",
    "@types/compression": "0.0.36",
    "@types/debug": "4.1.2",
    "@types/express": "4.16.1",
    "@types/jest": "24.0.6",
    "@types/node": "11.9.5",
    "@types/puppeteer": "1.12.1",
    "jest": "24.1.0",
    "ts-jest": "24.0.0",
    "tslint": "5.13.0",
    "typescript": "3.3.3333"
  }
}
