{
  "name": "rendergun",
  "version": "0.7.0",
  "description": "A pre-render service for client-side rendered websites",
  "repository": "https://github.com/goenning/rendergun",
  "main": "dist/index.js",
  "bin": {
    "rendergun": "dist/index.js"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsc && DEBUG=http,renderer:*,error,cache NODE_ENV=development node dist/index.js",
    "test": "jest",
    "lint": "tslint --project .",
    "build": "tsc",
    "prepublishOnly": "npm run lint && npm run test && npm run build"
  },
  "keywords": [
    "ssr",
    "puppeteer",
    "server-side-rendering",
    "docker"
  ],
  "author": "goenning <me@goenning.net>",
  "license": "MIT",
  "dependencies": {
    "compression": "1.7.4",
    "debug": "4.1.1",
    "express": "4.17.1",
    "lru-cache": "5.1.1",
    "puppeteer": "2.0.0"
  },
  "devDependencies": {
    "@types/lru-cache": "5.1.0",
    "@types/compression": "1.0.1",
    "@types/debug": "4.1.5",
    "@types/express": "4.17.1",
    "@types/jest": "24.0.21",
    "@types/node": "12.12.0",
    "@types/puppeteer": "1.20.2",
    "jest": "24.9.0",
    "ts-jest": "24.1.0",
    "tslint": "5.20.0",
    "typescript": "3.6.4"
  }
}
